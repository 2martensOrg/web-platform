{
    "name": "2martens/web-platform",
    "type": "library",
    "description": "The 2martens Web Platform",
    "keywords": ["framework"],
    "homepage": "https://github.com/2martens/web-platform",
    "license": "MIT",
    "authors": [
        {
            "name": "Jim Martens",
            "email": "github@2martens.de"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/symfony": "~3.0",
        "twig/twig": "~1.23|~2.0",
        "twig/extensions": "~1.3",
        "doctrine/common": "~2.4",
        "psr/log": "~1.0",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "sonata-project/block-bundle": "~2.3",
        "knplabs/knp-menu-bundle": "~2.1"
    },
    "require-dev": {
        "doctrine/data-fixtures": "~1.1",
        "doctrine/orm": "~2.4",
        "doctrine/mongodb-odm": "~1.0",
        "doctrine/mongodb-odm-bundle": "dev-master",
        "phpunit/phpunit": "~5.2",
        "squizlabs/php_codesniffer": "~2.5",
        "phpmd/phpmd": "~2.3",
        "symfony/assetic-bundle": "~2.7",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.9"
    },
    "autoload": {
        "psr-4": {
            "TwoMartens\\": "src/TwoMartens/"
        },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
